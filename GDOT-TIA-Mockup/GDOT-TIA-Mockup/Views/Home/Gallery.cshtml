@{
    Layout = "~/Views/_Shared/_Layout.cshtml";
	ViewBag.Title = "Gallery";
}

<h2>Photo Albums</h2>

@{ 
	foreach (var album in ViewBag.albumList) {
	   <p><a href="/Home/SlideShow/@album.PhotosetId">@album.Title</a> - Last Photo Taken @album.PrimaryPhoto.DateTaken<br />@album.Description</p>
	
	}
   
}
